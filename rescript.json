{
    "name": "rescript-parcel-rsc",
    "sources": [
        {
            "dir": "pages",
            "subdirs": true
        },
        {
            "dir": "src",
            "subdirs": true
        }
    ],
    "package-specs": [
        {
            "module": "esmodule",
            "in-source": true
        }
    ],
    "suffix": ".mjs",
    "jsx": {
        "version": 4
    },
    "bs-dependencies": [
        "@rescript/react",
        "@rescript/webapi"
    ],
    "bsc-flags": [
        "-open WebAPI.Global"
    ]
}